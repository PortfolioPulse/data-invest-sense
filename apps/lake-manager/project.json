{
  "name": "lake-manager",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/lake-manager",
  "targets": {
    "build": {
      "executor": "@nx-go/nx-go:build",
      "options": {
        "outputPath": "dist/apps/lake-manager",
        "main": "apps/lake-manager/cmd/server/main.go"
      }
    },
    "serve": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "main": "apps/lake-manager/cmd/server/main.go"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "go-tidy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go mod tidy",
        "cwd": "apps/lake-manager"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run cmd/server/main.go cmd/server/wire_gen.go",
        "cwd": "apps/lake-manager"
      }
    },
    "wire": {
      "executor": "nx:run-commands",
      "options": {
        "command": "wire",
        "cwd": "apps/lake-manager/cmd/server"
      }
    },
    "protoc": {
      "executor": "nx:run-commands",
      "options": {
        "command": "protoc --go_out=. --go-grpc_out=. internal/infra/grpc/protofiles/schema_input.proto",
        "cwd": "apps/lake-manager"
      }
    },
      "graphql-init": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go run github.com/99designs/gqlgen init",
          "cwd": "apps/lake-manager"
        }
      },
      "graphql": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go run github.com/99designs/gqlgen generate",
          "cwd": "apps/lake-manager"
        }
      }
  },
  "tags": []
}
